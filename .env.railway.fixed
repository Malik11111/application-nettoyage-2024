# Variables d'environnement pour Railway - CORRIGÉ
NODE_ENV=production
JWT_SECRET=super-secret-jwt-key-change-me-in-production-123456789
JWT_EXPIRES_IN=24h
BCRYPT_ROUNDS=12
PORT=3001

# Database - Railway injecte automatiquement DATABASE_URL
# DATABASE_URL sera ajouté automatiquement par Railway PostgreSQL

# CORS - Plus permissif pour Railway
CORS_ORIGIN=*

# Rate Limiting - Plus souple pour démarrage
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=500
AUTH_RATE_LIMIT_MAX=20
API_RATE_LIMIT_MAX=200

# Désactiver HTTPS strict pour Railway
DISABLE_HTTPS_REDIRECT=true